const express = require('express');
const app = express();
const port = 3000;


app.use(express.json());

app.get('/', (req, res) =>{
    let dados = {texto: 'ok!'};
    res.render('principal', dados)
})






app.listen(port, () => {
    console.log(`Servidor em execução: http://localhost:${port}`);
});